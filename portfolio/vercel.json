{
  "headers": [
    {
      "source": "/api/verify-code",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" } 
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/verify-code",
      "methods": ["POST"],
      "middleware": "ip-rate-limit",
      "options": {
        "limit": 10,
        "period": 60 
      }
    }
  ]
}